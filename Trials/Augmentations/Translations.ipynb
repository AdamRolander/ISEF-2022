   "source": [
    "# Shifted randomly up to 8% of 224 pixels vertically and horizontally\n",
    "from keras.preprocessing.image import ImageDataGenerator\n",
    "\n",
    "augment = ImageDataGenerator(\n",
    "    rescale = 1./255,\n",
    "    width_shift_range=0.08,\n",
    "    height_shift_range=0.08\n",
    ")"
   ]
  }
